{
    "name": "curious-people/curio",
    "description": "Social curation platform",
    "keywords": [
        "social media",
        "bookmark",
        "Api",
        "REST",
        "oAuth2"
    ],
    "license": "Curious People Ltd",
    "type": "platform",
    "authors": [
        {
            "name": "Jayenne Montana",
            "email": "jayenne@homtail.com",
            "homepage": "https://jayenne.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "algolia/algoliasearch-client-php": "^2.7",
        "algolia/scout-extended": "^1.10",
        "binarytorch/larecipe": "^2.4",
        "chojnicki/video-platforms-parser": "^1.2",
        "cybercog/laravel-love": "^8.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "graham-campbell/markdown": "^13.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "intervention/imagecache": "^2.5",
        "jayenne/laravel-locale-hero": "dev-master",
        "kirschbaum-development/nova-inline-relationship": "^0.3.0",
        "laravel/framework": "^8.14",
        "laravel/horizon": "^5.2",
        "laravel/nova": "^3.9",
        "laravel/sanctum": "^2.6",
        "laravel/scout": "^8.3",
        "laravel/socialite": "^5.0",
        "laravel/telescope": "^4.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "league/csv": "^9.6",
        "livewire/livewire": "^1.3",
        "melihovv/laravel-log-viewer": "^7.0",
        "nothingworks/blade-svg": "^0.3.4",
        "orchestra/testbench": "^6.2",
        "overtrue/laravel-follow": "^2.1",
        "overtrue/laravel-subscribe": "^1.0",
        "predis/predis": "^1.1",
        "shweshi/opengraph": "^1.0",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-fractal": "^5.8",
        "spatie/laravel-medialibrary": "^8.10",
        "spatie/laravel-model-status": "^1.10",
        "spatie/laravel-query-builder": "^3.3",
        "spatie/laravel-tags": "^3.0",
        "spatie/laravel-twitter-streaming-api": "^1.4",
        "squizlabs/php_codesniffer": "^3.5",
        "thujohn/twitter": "^2.2",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "beyondcode/laravel-query-detector": "^1.4",
        "facade/ignition": "^2.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "fakerphp/faker": "^1.9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
